{
    "media" : {
        "path": "media/",
        "landscape": true,
        "portrait": false
    },
    "autorefresh": {
        "enable": true,
        "time": "1:00:00"
    },
    "position": {
        "x": 0,
        "y": 0
    },
    "cache": {
        "path": "cache/",
        "depth": 10
    },
    "browserdata": {
        "path": "browserdata/"
    },
    "metadata": {
        "path": "metadata.json"
    },
    "port": 5000
}
